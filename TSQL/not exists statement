SELECT * FROM AVAGeminiOrderLine OL
WHERE NOT EXISTS 
(
    SELECT * FROM AVAGeminiHidesUsedByOrderLine HOL
		JOIN AVAGeminiHideLine HOL2
		ON	HOL2.GeminiOrderId		= HOL.GeminiOrderId
		AND	HOL2.HideId				= HOL.HideId
		AND	HOL2.Status				!= 3

	WHERE HOL.GeminiOrderId		= OL.GeminiOrderId
    AND		HOL.Marker			= OL.Marker
	AND		HOL.ProdId			= OL.ProdId
)

Ex. 2

SELECT * FROM LEDGERJOURNALTRANS T1 
WHERE (((T1.DATAAREAID=N'l501')) 
AND (T1.JOURNALNUM='J000000004')) 
AND NOT (EXISTS (SELECT TOP 10 'x' FROM GENERALJOURNALENTRY T2 WHERE ((((T2.SUBLEDGERVOUCHER=T1.VOUCHER) AND (T2.ACCOUNTINGDATE=T1.TRANSDATE)) ))))
